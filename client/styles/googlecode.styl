.hljs, .topic code
  display block;
  overflow-x auto;
  padding 0.5em 2em;
  color black;
  font-size 80%
  background-color rgba(0, 0, 0, 0.02)
  border-left solid 5px #808942
  white-space pre


.hljs-comment
  color gray
.hljs-quote 
  color #C00;


.hljs-keyword,
.hljs-selector-tag,
.hljs-section,
.hljs-title,
.hljs-name 
  color #00F;


.hljs-variable,
.hljs-template-variable 
  color #AA0;


.hljs-string,
.hljs-selector-attr,
.hljs-selector-pseudo,
.hljs-regexp 
  color #0C0;


.hljs-literal,
.hljs-symbol,
.hljs-bullet,
.hljs-meta,
.hljs-number,
.hljs-link 
  color #0AA;


.hljs-title,
.hljs-doctag,
.hljs-type,
.hljs-attr,
.hljs-built_in,
.hljs-builtin-name,
.hljs-params 
  color #A0A;


.hljs-attribute,
.hljs-subst 
  color #000;


.hljs-formula 
  background-color #eee;
  font-style italic;


.hljs-selector-id,
.hljs-selector-class 
  color #9B703F


.hljs-addition 
  background-color #baeeba;


.hljs-deletion 
  background-color #ffc8bd;


.hljs-doctag,
.hljs-strong 
  font-weight bold;


.hljs-emphasis 
  font-style italic;

